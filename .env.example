# Copy this file to .env and fill in your actual values

# OpenAI API Configuration (Required for AI features)
OPENAI_API_KEY=your_openai_api_key_here

# GitHub Configuration (Optional - for better API rate limits)
GITHUB_TOKEN=your_github_personal_access_token_here

# Zyte Proxy Configuration (Optional - for advanced captcha handling)
ZYTE_API_KEY=api key
ZYTE_PROXY_URL=http://api.zyte.com:8011
ZYTE_ENABLED=True

# Redis Configuration (Required for job queue and caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Cache Configuration (Optional - defaults provided)
CACHE_TTL=3600
JOB_STATUS_TTL=86400

# Search Configuration (Optional - defaults provided)
MAX_PROFILES=20
REQUEST_DELAY=2



# Browser Configuration (Optional - defaults provided)
HEADLESS=False
BROWSER_TIMEOUT=30 